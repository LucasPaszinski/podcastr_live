    <link href="https://fonts.googleapis.com/css2?family=Inter&family=Lexend:wght@500;600&display=swap" rel="stylesheet" />
    <%# <link phx-track-static rel="stylesheet" href="<%= Routes.static_path(@conn, "/css/app.css") "/> %>
